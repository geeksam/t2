<h1>Editing time_block</h1>

<% form_for(@time_block) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'form', :locals => {:f => f} %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= separated_group do |g|
  g << link_to('Show', @time_block)
  g << link_to('Back', time_blocks_path)
end %>
